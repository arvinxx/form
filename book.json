{
    "root": "./docs",
    "author": "Arvinx",
    "language": "zh-hans",
    "plugins": [
        "disqus",
        "github",
        "expandable-chapters-small",
        "splitter",
        "-sharing",
        "sharing-plus",
    "local-pagefooter",
            "ga",
        "anchors",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "arvinx"
        },
        "github": {
            "url": "https://github.com/arvinxx"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": ["facebook", "google", "twitter", "instapaper", "pocket"]
        },
        "local-pagefooter": {
            "copyright": "Copyright &copy ArvinX",
            "islocal" : true,
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "ga": {
            "token": "UA-110579858-1"
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": true,
            "isRewritePageTitle": false,
            "float": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        }
    },
    "theme-default": {
        "showLevel": true
    }
}

{
    "root": "./docs",
    "author": "Arvinx",
    "language": "zh-hans",
    "plugins": [
        "disqus",
        "github",
        "expandable-chapters-small",
        "splitter",
        "-sharing",
        "sharing-plus"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "arvinx"
        },
        "github": {
            "url": "https://github.com/arvinxx"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "instapaper",
                "pocket"
            ]
        }
    },
    "theme-default": {
        "showLevel": true
    }
}
